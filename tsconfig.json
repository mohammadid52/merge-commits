{
  "compilerOptions": {
    "target": "es5",

    "lib": ["dom", "dom.iterable", "esnext"],

    "outDir": "build",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@contexts/*": ["contexts/*"],
      "@customGraphql/*": ["customGraphql/*"],
      "@customHooks/*": ["customHooks/*"],
      "@graphql/*": ["graphql/*"],
      "@interfaces/*": ["interfaces/*"],
      "@style/*": ["style/*"],
      "@utilities/*": ["utilities/*"],
      "@atoms/*": ["components/Atoms/*"],
      "@molecules/*": ["components/Molecules/*"],
      "@uiComponents/*": ["components/Lesson/UniversalLessonBuilder/UI/UIComponents/*"],
      "@UlbUI/*": ["components/Lesson/UniversalLessonBuilder/UI/*"],
      "@UlbBlocks/*": ["components/Lesson/UniversalLessonBlockComponents/Blocks/*"],
      "@dictionary/*": ["dictionary/*"]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "jsx": "react"
  },
  "include": ["./src/**/*"],
  "exclude": ["build", "bin", "routes", "public", "node-modules", "src/aws-exports.js"]
}
